{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["src", "src/README.md", "src/liveoak", "src/liveoak/error"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false
        }
    }
}

